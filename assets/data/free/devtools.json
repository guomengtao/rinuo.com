  "examples": {
    "get_repository": {
      "description": "获取仓库信息",
      "endpoint": "GET https://api.github.com/repos/{owner}/{repo}",
      "response": {
        "id": 123456,
        "name": "repository-name",
        "full_name": "owner/repository-name",
        "description": "Repository description",
        "html_url": "https://github.com/owner/repository-name",
        "stargazers_count": 100
      }
    },
    "lint_code": {
      "description": "代码质量检测",
      "endpoint": "ESLint CLI",
      "code": "eslint --format json src/",
      "response": {
        "filePath": "src/index.js",
        "messages": [
          {
            "ruleId": "no-console",
            "severity": 1,
            "message": "Unexpected console statement."
          }
        ]
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "@octokit/rest",
