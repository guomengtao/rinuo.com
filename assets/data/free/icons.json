{
  "category": "图标服务",
  "description": "提供图标库、图标搜索、图标生成、SVG图标等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 5,
  "apis": [
    {
      "id": "icons-001",
      "name": "Font Awesome",
      "description": "流行的图标字体库，提供1600+免费图标",
      "endpoint": "CDN链接",
      "method": "CSS引入",
      "parameters": {
        "version": "版本号",
        "style": "样式类型",
        "cdn": "CDN提供商"
      },
      "response_format": "CSS字体",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://fontawesome.com/docs",
      "website": "https://fontawesome.com",
      "tags": ["图标字体", "1600+图标", "完全免费"]
    },
    {
      "id": "icons-002",
      "name": "Feather Icons",
      "description": "简洁优雅的开源图标集，支持多种格式",
      "endpoint": "https://feathericons.com/api",
      "method": "GET",
      "parameters": {
        "name": "图标名称",
        "size": "图标尺寸",
        "stroke": "线条宽度",
        "color": "图标颜色"
      },
      "response_format": "SVG/PNG",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://feathericons.com/",
      "website": "https://feathericons.com",
      "tags": ["简洁设计", "开源", "多格式"]
    },
    {
      "id": "icons-003",
      "name": "Iconify",
      "description": "统一图标框架，整合100+图标集",
      "endpoint": "https://api.iconify.design",
      "method": "GET",
      "parameters": {
        "icon": "图标标识",
        "width": "宽度",
        "height": "高度",
        "color": "颜色",
        "flip": "翻转"
      },
      "response_format": "SVG",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://iconify.design/docs/api/",
      "website": "https://iconify.design",
      "tags": ["统一框架", "100+图标集", "完全免费"]
    },
    {
      "id": "icons-004",
      "name": "Heroicons",
      "description": "Tailwind CSS团队设计的精美图标集",
      "endpoint": "https://heroicons.com",
      "method": "下载/CDN",
      "parameters": {
        "style": "样式类型",
        "size": "尺寸大小",
        "type": "图标类型"
      },
      "response_format": "SVG/React组件",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://heroicons.com/",
      "website": "https://heroicons.com",
      "tags": ["Tailwind设计", "精美图标", "完全免费"]
    },
    {
      "id": "icons-005",
      "name": "Material Icons",
      "description": "Google Material Design图标库",
      "endpoint": "https://fonts.googleapis.com/icon",
      "method": "CSS引入",
      "parameters": {
        "family": "图标字体",
        "weight": "字体粗细",
        "style": "字体样式"
      },
      "response_format": "字体图标",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://developers.google.com/fonts/docs/material_icons",
      "website": "https://fonts.google.com/icons",
      "tags": ["Material Design", "Google官方", "完全免费"]
    }
  ],
  "examples": {
    "get_icon": {
      "description": "获取图标",
      "endpoint": "GET https://api.iconify.design/mdi:home.svg?width=24&height=24&color=#3b82f6",
      "response": "<svg width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"#3b82f6\">...</svg>"
    },
    "search_icons": {
      "description": "搜索图标",
      "endpoint": "GET https://api.iconify.design/search?query=home",
      "response": {
        "icons": [
          {
            "prefix": "mdi",
            "name": "home",
            "total": 1
          }
        ]
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "@iconify/react",
      "install": "npm install @iconify/react",
      "usage": "import { Icon } from '@iconify/react';"
    },
    {
      "language": "Vue",
      "package": "@iconify/vue",
      "install": "npm install @iconify/vue",
      "usage": "import { Icon } from '@iconify/vue';"
    },
    {
      "language": "React",
      "package": "react-feather",
      "install": "npm install react-feather",
      "usage": "import { Home } from 'react-feather';"
    }
  ]
}
