      "description": "搜索新闻",
      "endpoint": "GET https://newsapi.org/v2/everything?q=technology&apiKey=YOUR_KEY",
      "response": {
        "status": "ok",
        "totalResults": 100,
        "articles": [
          {
            "source": {"name": "TechCrunch"},
            "author": "Author Name",
            "title": "Article Title",
            "description": "Article description",
            "url": "https://example.com/article",
            "publishedAt": "2023-01-20T10:00:00Z"
          }
        ]
      }
    },
    "get_top_headlines": {
      "description": "获取头条新闻",
      "endpoint": "GET https://newsapi.org/v2/top-headlines?country=us&apiKey=YOUR_KEY",
