  "examples": {
    "create_document": {
      "description": "创建Google文档",
      "endpoint": "POST https://docs.googleapis.com/v1/documents",
      "response": {
        "documentId": "doc_id",
        "title": "Untitled document",
        "revisionId": "revision_id"
      }
    },
    "convert_document": {
      "description": "转换文档格式",
      "endpoint": "Pandoc CLI",
      "code": "pandoc input.md -o output.pdf",
      "response": "转换后的PDF文档"
    },
    "create_notion_page": {
      "description": "创建Notion页面",
      "endpoint": "POST https://api.notion.com/v1/pages",
      "response": {
        "id": "page_id",
        "url": "https://notion.so/page_id",
        "created_time": "2023-01-20T10:00:00.000Z"
      }
    }
