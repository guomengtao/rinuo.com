      "tags": ["微软云", "多种Blob", "企业级"]
    },
    {
      "id": "storage-004",
      "name": "Cloudinary",
      "description": "专注于媒体文件的云存储服务，提供图像和视频的优化、转换功能",
      "endpoint": "https://api.cloudinary.com/v1_1/{cloud_name}/image/upload",
      "method": "POST",
      "parameters": {
        "api_key": "API密钥",
        "timestamp": "时间戳",
        "signature": "签名",
        "file": "文件数据",
        "public_id": "公共ID"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://cloudinary.com/documentation",
      "website": "https://cloudinary.com",
      "tags": ["媒体存储", "图像优化", "视频转换"]
    },
    {
      "id": "storage-005",
      "name": "Supabase Storage",
      "description": "开源的存储服务，基于PostgreSQL和S3兼容的存储后端",
      "endpoint": "https://{project}.supabase.co/storage/v1/object/{bucket}/{path}",
      "method": "GET/POST/PUT/DELETE",
      "parameters": {
        "Authorization": "Bearer {token}",
