  "apis": [
    {
      "id": "storage-001",
      "name": "AWS S3",
      "description": "亚马逊的对象存储服务，提供高可用性、高扩展性的存储解决方案",
      "endpoint": "https://{bucket}.s3.{region}.amazonaws.com/{key}",
      "method": "GET/PUT/POST/DELETE",
      "parameters": {
        "Authorization": "AWS4-HMAC-SHA256",
        "Content-Type": "内容类型",
        "x-amz-acl": "访问控制",
        "x-amz-meta-*": "自定义元数据"
      },
      "response_format": "Binary/XML",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://docs.aws.amazon.com/s3/",
      "website": "https://aws.amazon.com/s3/",
      "tags": ["对象存储", "高可用性", "全球服务"]
    },
    {
      "id": "storage-002",
      "name": "Google Cloud Storage",
      "description": "谷歌云存储服务，提供统一的存储API和多种存储类别",
      "endpoint": "https://storage.googleapis.com/{bucket}/{object}",
      "method": "GET/PUT/POST/DELETE",
      "parameters": {
        "Authorization": "Bearer {access_token}",
        "Content-Type": "内容类型",
        "x-goog-meta-*": "自定义元数据",
        "x-goog-acl": "访问控制"
      },
      "response_format": "Binary/JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://cloud.google.com/storage/docs",
      "website": "https://cloud.google.com/storage",
      "tags": ["云存储", "统一API", "多种类别"]
    },
    {
      "id": "storage-003",
      "name": "Azure Blob Storage",
      "description": "微软的云存储服务，支持块、页、追加三种blob类型",
      "endpoint": "https://{account}.blob.core.windows.net/{container}/{blob}",
      "method": "GET/PUT/POST/DELETE",
      "parameters": {
        "Authorization": "SharedKey",
        "x-ms-version": "API版本",
        "x-ms-blob-type": "Blob类型",
        "x-ms-meta-*": "自定义元数据"
      },
      "response_format": "Binary/XML",
      "rate_limit": "根据套餐",
