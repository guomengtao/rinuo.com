  "examples": {
    "upload_file": {
      "description": "上传文件到S3",
      "endpoint": "PUT https://{bucket}.s3.{region}.amazonaws.com/{key}",
      "request_body": "文件二进制数据",
      "headers": {
        "Authorization": "AWS4-HMAC-SHA256 Credential=...",
        "Content-Type": "image/jpeg",
        "x-amz-acl": "public-read"
      },
      "response": {
        "ETag": "\"d41d8cd98f00b204e9800998ecf8427e\""
      }
    },
    "download_file": {
      "description": "从Cloudinary下载图像",
      "endpoint": "GET https://res.cloudinary.com/{cloud_name}/image/upload/{public_id}",
      "response": "图像二进制数据"
    }
