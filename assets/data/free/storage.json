      "method": "GET/PUT/POST/DELETE",
      "parameters": {
        "Authorization": "AWS4-HMAC-SHA256",
        "Content-Type": "内容类型",
        "x-amz-acl": "访问控制",
        "x-amz-meta-*": "自定义元数据"
      },
      "response_format": "Binary/XML",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://docs.min.io/",
      "website": "https://min.io",
      "tags": ["开源", "S3兼容", "私有部署"]
    }
