    },
    {
      "id": "storage-006",
      "name": "Firebase Storage",
      "description": "谷歌的移动和Web应用存储服务，支持实时同步和离线访问",
      "endpoint": "https://firebasestorage.googleapis.com/v0/b/{bucket}/o/{path}",
      "method": "GET/POST/PUT/DELETE",
      "parameters": {
        "Authorization": "Bearer {id_token}",
        "Content-Type": "内容类型",
        "uploadType": "上传类型",
