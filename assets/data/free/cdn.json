  "examples": {
    "purge_cache": {
      "description": "Purge cache from Cloudflare",
      "endpoint": "POST https://api.cloudflare.com/client/v4/zones/{zone_id}/purge_cache",
      "request_body": {
        "files": [
          "https://example.com/image1.jpg",
          "https://example.com/image2.jpg"
        ]
      },
      "response": {
        "success": true,
        "result": {
          "id": "purge_cache_id"
        }
      }
    },
    "create_invalidation": {
      "description": "Create CloudFront invalidation",
      "endpoint": "POST https://cloudfront.amazonaws.com/2020-05-31/distribution/{id}/invalidation",
      "request_body": {
        "Paths": {
          "Quantity": 1,
          "Items": ["/*"]
        },
        "CallerReference": "unique_reference"
      },
      "response": {
        "Id": "invalidation_id",
        "Status": "InProgress"
      }
    },
    "image_delivery": {
      "description": "Fetch optimized image via Cloudinary",
      "endpoint": "GET https://res.cloudinary.com/demo/image/upload/w_400,h_300,c_fill/sample.jpg",
      "response": "Returns an optimized 400x300 image"
    },
    "video_upload": {
      "description": "Upload a video asset via Mux API",
      "endpoint": "POST https://api.mux.com/video/v1/assets",
      "request_body": {
        "input": "https://example.com/video.mp4",
        "playback_policy": ["public"]
      },
      "response": {
        "data": {
          "id": "video_asset_id",
          "status": "ready"
        }
      }
    }
