  "examples": {
    "purge_cache": {
      "description": "清除CDN缓存",
      "endpoint": "POST https://api.cloudflare.com/client/v4/zones/{zone_id}/purge_cache",
      "request_body": {
        "files": [
          "https://example.com/image1.jpg",
          "https://example.com/image2.jpg"
        ]
      },
      "response": {
        "success": true,
        "result": {
          "id": "purge_cache_id"
        }
      }
    },
    "create_invalidation": {
      "description": "创建CloudFront失效请求",
      "endpoint": "POST https://cloudfront.amazonaws.com/2020-05-31/distribution/{id}/invalidation",
      "request_body": {
        "Paths": {
          "Quantity": 1,
          "Items": ["/*"]
        },
        "CallerReference": "unique_reference"
      },
      "response": {
        "Id": "invalidation_id",
        "Status": "InProgress"
      }
    }
