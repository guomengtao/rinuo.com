{
  "category": "Serverless服务",
  "description": "提供无服务器计算、函数即服务等API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 5,
  "apis": [
    {
      "id": "serverless-001",
      "name": "AWS Lambda",
      "description": "亚马逊的无服务器计算服务，支持多种编程语言",
      "endpoint": "https://lambda.{region}.amazonaws.com/2015-03-31/functions/{function_name}/invocations",
      "method": "POST",
      "parameters": {
        "X-Amz-Date": "日期",
        "Authorization": "AWS4-HMAC-SHA256",
        "Content-Type": "application/json",
        "Payload": "函数参数"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://docs.aws.amazon.com/lambda/",
      "website": "https://aws.amazon.com/lambda/",
      "tags": ["无服务器", "多种语言", "AWS生态"]
    },
    {
      "id": "serverless-002",
      "name": "Vercel Functions",
      "description": "Vercel平台的Serverless函数服务，专为前端应用优化",
      "endpoint": "https://{project}.vercel.app/api/{function_name}",
      "method": "GET/POST",
      "parameters": {
        "Content-Type": "application/json",
        "body": "请求体",
        "query": "查询参数"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://vercel.com/docs/functions",
      "website": "https://vercel.com",
      "tags": ["前端优化", "自动部署", "免费套餐"]
    },
    {
      "id": "serverless-003",
      "name": "Netlify Functions",
      "description": "Netlify的Serverless函数服务，支持多种运行时",
      "endpoint": "https://{site}.netlify.app/.netlify/functions/{function_name}",
      "method": "GET/POST",
      "parameters": {
        "Content-Type": "application/json",
        "body": "请求体",
        "query": "查询参数"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://docs.netlify.com/functions/",
      "website": "https://www.netlify.com",
      "tags": ["多种运行时", "自动部署", "免费套餐"]
    },
    {
      "id": "serverless-004",
      "name": "Cloudflare Workers",
      "description": "Cloudflare的边缘计算平台，全球分布式部署",
      "endpoint": "https://{worker}.{subdomain}.workers.dev",
      "method": "GET/POST",
      "parameters": {
        "Content-Type": "application/json",
        "body": "请求体",
        "headers": "请求头"
      },
      "response_format": "JSON",
      "rate_limit": "100,000次/天(免费)",
      "is_free": true,
      "documentation": "https://developers.cloudflare.com/workers/",
      "website": "https://workers.cloudflare.com",
      "tags": ["边缘计算", "全球分布", "免费套餐"]
    },
    {
      "id": "serverless-005",
      "name": "Supabase Edge Functions",
      "description": "Supabase的边缘函数服务，基于Deno运行时",
      "endpoint": "https://{project}.supabase.co/functions/v1/{function_name}",
      "method": "POST",
      "parameters": {
        "Authorization": "Bearer {anon_key}",
        "Content-Type": "application/json",
        "body": "请求体"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://supabase.com/docs/guides/functions",
      "website": "https://supabase.com",
      "tags": ["Deno运行时", "PostgreSQL集成", "开源"]
    }
  ],
  "examples": {
    "invoke_function": {
      "description": "调用Lambda函数",
      "endpoint": "POST https://lambda.us-east-1.amazonaws.com/2015-03-31/functions/my-function/invocations",
      "request_body": {
        "name": "World",
        "message": "Hello"
      },
      "response": {
        "statusCode": 200,
        "body": "Hello World"
      }
    },
    "edge_function": {
      "description": "调用Cloudflare Worker",
      "endpoint": "POST https://my-worker.username.workers.dev",
      "request_body": {
        "data": "test"
      },
      "response": {
        "result": "processed"
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "aws-sdk",
      "install": "npm install aws-sdk",
      "usage": "const AWS = require('aws-sdk');"
    },
    {
      "language": "Python",
      "package": "boto3",
      "install": "pip install boto3",
      "usage": "import boto3"
    }
  ]
