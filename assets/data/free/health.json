{
  "category": "健康服务",
  "description": "提供健康数据、医疗信息、健身追踪等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 4,
  "apis": [
    {
      "id": "health-001",
      "name": "Fitbit API",
      "description": "Fitbit可穿戴设备的健康数据API，提供运动和健康指标",
      "endpoint": "https://api.fitbit.com/1/user/-/{resource-path}.json",
      "method": "GET",
      "parameters": {
        "Authorization": "Bearer {access_token}",
        "date": "日期",
        "period": "时间段",
        "base-date": "基准日期"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://dev.fitbit.com/build/reference/web-api/",
      "website": "https://www.fitbit.com",
      "tags": ["可穿戴设备", "健康数据", "运动追踪"]
    },
    {
      "id": "health-002",
      "name": "Apple HealthKit",
      "description": "苹果健康数据平台，提供iOS设备的健康信息",
      "endpoint": "HealthKit Framework",
      "method": "iOS SDK",
      "parameters": {
        "HKObjectType": "健康数据类型",
        "predicate": "查询条件",
        "sortDescriptors": "排序描述",
        "limit": "数量限制"
      },
      "response_format": "HealthKit Objects",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://developer.apple.com/health-fitness/",
      "website": "https://developer.apple.com",
      "tags": ["苹果生态", "iOS专用", "健康数据"]
    },
    {
      "id": "health-003",
      "name": "Google Fit API",
      "description": "谷歌健康平台，提供Android设备的健康数据",
      "endpoint": "https://www.googleapis.com/fitness/v1/users/me/dataset:aggregate",
      "method": "POST",
      "parameters": {
        "Authorization": "Bearer {access_token}",
        "aggregateBy": "聚合方式",
        "bucketByTime": "时间分桶",
        "startTimeMillis": "开始时间",
        "endTimeMillis": "结束时间"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://developers.google.com/fit",
      "website": "https://developers.google.com/fit",
      "tags": ["谷歌生态", "Android", "免费使用"]
    },
    {
      "id": "health-004",
      "name": "WHO API",
      "description": "世界卫生组织健康数据API，提供全球健康统计信息",
      "endpoint": "https://ghoapi.azureedge.net/api/{indicator}",
      "method": "GET",
      "parameters": {
        "format": "响应格式",
        "filter": "过滤条件",
        "select": "选择字段",
        "orderby": "排序方式"
      },
      "response_format": "JSON",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://www.who.int/data/gho/info/gho-odata-api",
      "website": "https://www.who.int",
      "tags": ["世界卫生组织", "全球数据", "完全免费"]
    }
  ],
  "examples": {
    "get_activity": {
      "description": "获取活动数据",
      "endpoint": "GET https://api.fitbit.com/1/user/-/activities/steps/date/today/1d.json",
      "response": {
        "activities-steps": [
          {
            "dateTime": "2023-01-20",
            "value": "8432"
          }
        ]
      }
    },
    "get_health_stats": {
      "description": "获取健康统计",
      "endpoint": "POST https://www.googleapis.com/fitness/v1/users/me/dataset:aggregate",
      "request_body": {
        "aggregateBy": [{"dataTypeName": "com.google.step_count.delta"}],
        "bucketByTime": {"durationMillis": 86400000},
        "startTimeMillis": "1640995200000",
        "endTimeMillis": "1641081600000"
      },
      "response": {
        "bucket": [
          {
            "startTimeMillis": "1640995200000",
            "endTimeMillis": "1641081600000",
            "dataset": [{"point": [{"value": [{"intVal": 8432}]}]}]
          }
        ]
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "fitbit-node",
      "install": "npm install fitbit-node",
      "usage": "const FitbitApiClient = require('fitbit-node');"
    },
    {
      "language": "Python",
      "package": "google-api-python-client",
      "install": "pip install google-api-python-client",
      "usage": "from googleapiclient.discovery import build"
    }
  ]
}
