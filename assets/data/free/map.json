{
  "category": "地图服务",
  "description": "提供地图、地理编码、路线规划等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 6,
  "apis": [
    {
      "id": "map-001",
      "name": "Google Maps",
      "description": "谷歌地图服务，提供全面的地图和地理功能",
      "endpoint": "https://maps.googleapis.com/maps/api/{service}/{output}",
      "method": "GET",
      "parameters": {
        "key": "API密钥",
        "address": "地址",
        "latlng": "坐标",
        "origin": "起点",
        "destination": "终点"
      },
      "response_format": "JSON/XML",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://developers.google.com/maps",
      "website": "https://developers.google.com/maps",
      "tags": ["谷歌地图", "全面功能", "全球覆盖"]
    },
    {
      "id": "map-002",
      "name": "OpenStreetMap",
      "description": "开源地图数据，提供免费的地图服务",
      "endpoint": "https://nominatim.openstreetmap.org/search",
      "method": "GET",
      "parameters": {
        "q": "搜索查询",
        "format": "响应格式",
        "limit": "结果数量",
        "countrycodes": "国家代码"
      },
      "response_format": "JSON/XML",
      "rate_limit": "1次/秒",
      "is_free": true,
      "documentation": "https://nominatim.org/release-docs/develop/api/Overview/",
      "website": "https://www.openstreetmap.org",
      "tags": ["开源", "免费", "社区驱动"]
    },
    {
      "id": "map-003",
      "name": "Mapbox",
      "description": "现代地图平台，提供自定义地图和导航服务",
      "endpoint": "https://api.mapbox.com/geocoding/v5/mapbox.places/{query}.json",
      "method": "GET",
      "parameters": {
        "access_token": "访问令牌",
        "query": "搜索查询",
        "types": "地点类型",
        "bbox": "边界框",
        "limit": "结果数量"
      },
      "response_format": "JSON",
      "rate_limit": "100,000次/月(免费)",
      "is_free": true,
      "documentation": "https://docs.mapbox.com/",
      "website": "https://www.mapbox.com",
      "tags": ["现代平台", "自定义地图", "免费套餐"]
    },
    {
      "id": "map-004",
      "name": "Here Maps",
      "description": "诺基亚地图服务，提供高质量的地图和导航",
      "endpoint": "https://geocode.search.hereapi.com/v1/geocode",
      "method": "GET",
      "parameters": {
        "apiKey": "API密钥",
        "q": "搜索查询",
        "at": "中心点",
        "limit": "结果数量",
        "lang": "语言"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://developer.here.com/",
      "website": "https://www.here.com",
      "tags": ["诺基亚", "高质量", "专业服务"]
    },
    {
      "id": "map-005",
      "name": "Bing Maps",
      "description": "微软必应地图，提供全球地图和地理服务",
      "endpoint": "https://dev.virtualearth.net/REST/v1/Locations",
      "method": "GET",
      "parameters": {
        "key": "API密钥",
        "query": "搜索查询",
        "maxResults": "最大结果数",
        "userLocation": "用户位置",
        "userIp": "用户IP"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://docs.microsoft.com/en-us/bingmaps/",
      "website": "https://www.bingmapsportal.com",
      "tags": ["微软", "必应地图", "企业级"]
    },
    {
      "id": "map-006",
      "name": "TomTom",
      "description": "专业导航服务，提供路线规划和交通信息",
      "endpoint": "https://api.tomtom.com/search/2/geocode/{query}.json",
      "method": "GET",
      "parameters": {
        "key": "API密钥",
        "query": "搜索查询",
        "limit": "结果数量",
        "countrySet": "国家集合",
        "idxSet": "索引集合"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://developer.tomtom.com/",
      "website": "https://www.tomtom.com",
      "tags": ["专业导航", "路线规划", "交通信息"]
    }
  ],
  "examples": {
    "geocode_address": {
      "description": "地理编码地址",
      "endpoint": "GET https://maps.googleapis.com/maps/api/geocode/json?address=1600+Amphitheatre+Parkway&key=YOUR_KEY",
      "response": {
        "results": [
          {
            "formatted_address": "1600 Amphitheatre Parkway, Mountain View, CA 94043, USA",
            "geometry": {
              "location": {
                "lat": 37.4224764,
                "lng": -122.0842499
              }
            }
          }
        ]
      }
    },
    "search_places": {
      "description": "搜索地点",
      "endpoint": "GET https://api.mapbox.com/geocoding/v5/mapbox.places/restaurant.json?access_token=YOUR_TOKEN",
      "response": {
        "features": [
          {
            "place_name": "Restaurant Name",
            "center": [-122.0842499, 37.4224764],
            "properties": {"category": "restaurant"}
          }
        ]
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "@googlemaps/js-api-loader",
      "install": "npm install @googlemaps/js-api-loader",
      "usage": "import { Loader } from '@googlemaps/js-api-loader';"
    },
    {
      "language": "Python",
      "package": "geopy",
      "install": "pip install geopy",
      "usage": "from geopy.geocoders import Nominatim"
    }
  ]
}
