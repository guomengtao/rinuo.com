{
  "category": "股票服务",
  "description": "提供股票数据、市场信息、金融分析等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 5,
  "apis": [
    {
      "id": "stock-001",
      "name": "Alpha Vantage",
      "description": "专业金融数据API，提供股票、外汇、加密货币数据",
      "endpoint": "https://www.alphavantage.co/query",
      "method": "GET",
      "parameters": {
        "function": "数据功能",
        "symbol": "股票代码",
        "apikey": "API密钥",
        "interval": "时间间隔",
        "outputsize": "输出大小"
      },
      "response_format": "JSON/CSV",
      "rate_limit": "5次/分钟(免费)",
      "is_free": true,
      "documentation": "https://www.alphavantage.co/documentation/",
      "website": "https://www.alphavantage.co",
      "tags": ["金融数据", "股票信息", "免费套餐"]
    },
    {
      "id": "stock-002",
      "name": "Yahoo Finance API",
      "description": "雅虎财经数据API，提供实时股票价格和市场数据",
      "endpoint": "https://query1.finance.yahoo.com/v8/finance/chart/{symbol}",
      "method": "GET",
      "parameters": {
        "symbol": "股票代码",
        "period1": "开始时间",
        "period2": "结束时间",
        "interval": "时间间隔",
        "includePrePost": "包含盘前盘后"
      },
      "response_format": "JSON",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://finance.yahoo.com/",
      "website": "https://finance.yahoo.com",
      "tags": ["雅虎财经", "实时数据", "完全免费"]
    },
    {
      "id": "stock-003",
      "name": "IEX Cloud",
      "description": "金融数据平台，提供股票、债券、期权等数据",
      "endpoint": "https://cloud.iexapis.com/stable/stock/{symbol}/quote",
      "method": "GET",
      "parameters": {
        "token": "API令牌",
        "symbol": "股票代码",
        "format": "响应格式"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://iexcloud.io/docs/api/",
      "website": "https://iexcloud.io",
      "tags": ["金融平台", "多资产", "专业数据"]
    },
    {
      "id": "stock-004",
      "name": "Finnhub",
      "description": "实时金融市场数据API，支持股票、外汇、加密货币",
      "endpoint": "https://finnhub.io/api/v1/quote",
      "method": "GET",
      "parameters": {
        "token": "API密钥",
        "symbol": "股票代码"
      },
      "response_format": "JSON",
      "rate_limit": "60次/分钟(免费)",
      "is_free": true,
      "documentation": "https://finnhub.io/docs/api",
      "website": "https://finnhub.io",
      "tags": ["实时数据", "多市场", "免费套餐"]
    },
    {
      "id": "stock-005",
      "name": "Polygon.io",
      "description": "企业级金融数据API，提供高质量市场数据",
      "endpoint": "https://api.polygon.io/v2/aggs/ticker/{ticker}/range/{multiplier}/{timespan}/{from}/{to}",
      "method": "GET",
      "parameters": {
        "apiKey": "API密钥",
        "ticker": "股票代码",
        "multiplier": "倍数",
        "timespan": "时间跨度",
        "from": "开始时间",
        "to": "结束时间"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://polygon.io/docs/",
      "website": "https://polygon.io",
      "tags": ["企业级", "高质量", "专业服务"]
    }
  ],
  "examples": {
    "get_stock_quote": {
      "description": "获取股票报价",
      "endpoint": "GET https://www.alphavantage.co/query?function=GLOBAL_QUOTE&symbol=AAPL&apikey=YOUR_KEY",
      "response": {
        "Global Quote": {
          "01. symbol": "AAPL",
          "02. open": "150.00",
          "03. high": "155.00",
          "04. low": "149.00",
          "05. price": "152.50",
          "06. volume": "1000000"
        }
      }
    },
    "get_historical_data": {
      "description": "获取历史数据",
      "endpoint": "GET https://query1.finance.yahoo.com/v8/finance/chart/AAPL?period1=1640995200&period2=1641081600&interval=1d",
      "response": {
        "chart": {
          "result": [
            {
              "timestamp": [1640995200, 1641081600],
              "indicators": {
                "quote": [{"close": [150.00, 152.50]}]
              }
            }
          ]
        }
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "Python",
      "package": "yfinance",
      "install": "pip install yfinance",
      "usage": "import yfinance as yf"
    },
    {
      "language": "JavaScript",
      "package": "yahoo-finance2",
      "install": "npm install yahoo-finance2",
      "usage": "const yahooFinance = require('yahoo-finance2');"
    }
  ]
}
