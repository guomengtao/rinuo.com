  "examples": {
    "create_instance": {
      "description": "创建VPS实例",
      "endpoint": "POST https://api.digitalocean.com/v2/droplets",
      "request_body": {
        "name": "my-server",
        "region": "nyc1",
        "size": "s-1vcpu-1gb",
        "image": "ubuntu-20-04-x64"
      },
      "response": {
        "droplet": {
          "id": 12345678,
          "name": "my-server",
          "status": "new",
          "created_at": "2025-01-20T10:00:00Z"
        }
      }
    },
    "list_instances": {
      "description": "列出所有VPS实例",
      "endpoint": "GET https://api.digitalocean.com/v2/droplets",
      "response": {
        "droplets": [
          {
            "id": 12345678,
            "name": "my-server",
            "status": "active",
            "region": {"name": "New York 1"}
          }
        ]
      }
    }
