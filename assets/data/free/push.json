{
  "category": "推送服务",
  "description": "提供推送通知、消息推送等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 5,
  "apis": [
    {
      "id": "push-001",
      "name": "Firebase Cloud Messaging",
      "description": "谷歌的跨平台推送通知服务，支持Android、iOS、Web",
      "endpoint": "https://fcm.googleapis.com/fcm/send",
      "method": "POST",
      "parameters": {
        "Authorization": "key={server_key}",
        "Content-Type": "application/json",
        "to": "目标设备",
        "notification": "通知内容",
        "data": "自定义数据"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://firebase.google.com/docs/cloud-messaging",
      "website": "https://firebase.google.com",
      "tags": ["跨平台", "谷歌服务", "免费使用"]
    },
    {
      "id": "push-002",
      "name": "OneSignal",
      "description": "全功能推送通知平台，支持多种平台和高级功能",
      "endpoint": "https://onesignal.com/api/v1/notifications",
      "method": "POST",
      "parameters": {
        "Authorization": "Basic {rest_api_key}",
        "Content-Type": "application/json",
        "app_id": "应用ID",
        "included_segments": "目标分段",
        "contents": "通知内容",
        "headings": "通知标题"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://documentation.onesignal.com/",
      "website": "https://onesignal.com",
      "tags": ["全功能", "多平台", "免费套餐"]
    },
    {
      "id": "push-003",
      "name": "Pushwoosh",
      "description": "企业级推送通知服务，提供高级分析和个性化功能",
      "endpoint": "https://cp.pushwoosh.com/json/1.3/createMessage",
      "method": "POST",
      "parameters": {
        "Content-Type": "application/json",
        "request": {
          "application": "应用代码",
          "auth": "认证令牌",
          "notifications": "通知数组"
        }
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://docs.pushwoosh.com/",
      "website": "https://pushwoosh.com",
      "tags": ["企业级", "高级分析", "个性化"]
    },
    {
      "id": "push-004",
      "name": "Airship",
      "description": "移动应用推送通知平台，专注于用户体验优化",
      "endpoint": "https://go.urbanairship.com/api/push/",
      "method": "POST",
      "parameters": {
        "Authorization": "Basic {app_key}:{master_secret}",
        "Content-Type": "application/json",
        "audience": "目标受众",
        "notification": "通知内容",
        "device_types": "设备类型"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://docs.airship.com/",
      "website": "https://www.airship.com",
      "tags": ["移动应用", "用户体验", "专业服务"]
    },
    {
      "id": "push-005",
      "name": "Pusher Push Notifications",
      "description": "基于Pusher的推送通知服务，简单易用",
      "endpoint": "https://{instance_id}.pushnotifications.pusher.com/publish_api/v1/instances/{instance_id}/publishes",
      "method": "POST",
      "parameters": {
        "Authorization": "Bearer {secret_key}",
        "Content-Type": "application/json",
        "interests": "兴趣数组",
        "publish": "发布内容",
        "apns": "iOS配置",
        "fcm": "Android配置"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://pusher.com/docs/beams",
      "website": "https://pusher.com",
      "tags": ["简单易用", "Pusher生态", "多平台"]
    }
  ],
  "examples": {
    "send_notification": {
      "description": "发送推送通知",
      "endpoint": "POST https://fcm.googleapis.com/fcm/send",
      "request_body": {
        "to": "device_token",
        "notification": {
          "title": "新消息",
          "body": "您有一条新消息"
        },
        "data": {
          "click_action": "OPEN_ACTIVITY",
          "id": "123"
        }
      },
      "response": {
        "message_id": "projects/myproject/messages/123456789"
      }
    },
    "send_to_segment": {
      "description": "发送到特定分段",
      "endpoint": "POST https://onesignal.com/api/v1/notifications",
      "request_body": {
        "app_id": "your_app_id",
        "included_segments": ["Subscribed Users"],
        "contents": {"en": "Hello from OneSignal!"},
        "headings": {"en": "Notification Title"}
      },
      "response": {
        "id": "notification_id",
        "recipients": 1000
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "firebase",
      "install": "npm install firebase",
      "usage": "import { getMessaging, sendMessage } from 'firebase/messaging';"
    },
    {
      "language": "Python",
      "package": "requests",
      "install": "pip install requests",
      "usage": "import requests"
    }
  ]
}
