  "examples": {
    "convert_video": {
      "description": "转换视频格式",
      "endpoint": "FFmpeg CLI",
      "code": "ffmpeg -i input.mp4 -c:v libx264 -c:a aac output.mp4",
      "response": "转换后的视频文件"
    },
    "upload_video": {
      "description": "上传视频到Cloudinary",
      "endpoint": "POST https://api.cloudinary.com/v1_1/{cloud_name}/video/upload",
      "response": {
        "public_id": "video_id",
        "url": "https://res.cloudinary.com/cloud_name/video/upload/v123/video_id.mp4",
        "duration": 30.5,
        "format": "mp4"
      }
    },
    "search_youtube": {
      "description": "搜索YouTube视频",
      "endpoint": "GET https://www.googleapis.com/youtube/v3/search?part=snippet&q=programming&key=YOUR_KEY",
      "response": {
        "items": [
          {
            "id": {"videoId": "video_id"},
            "snippet": {
              "title": "Video Title",
              "description": "Video Description"
            }
          }
        ]
      }
    }
