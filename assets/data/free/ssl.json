{
  "category": "SSL服务",
  "description": "提供SSL证书、HTTPS加密、证书管理等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 5,
  "apis": [
    {
      "id": "ssl-001",
      "name": "Let's Encrypt",
      "description": "免费SSL证书颁发机构，提供自动化证书管理",
      "endpoint": "https://acme-v02.api.letsencrypt.org/directory",
      "method": "POST",
      "parameters": {
        "protected": "JWS保护头",
        "payload": "JWS载荷",
        "signature": "JWS签名"
      },
      "response_format": "JSON",
      "rate_limit": "300次/3小时",
      "is_free": true,
      "documentation": "https://letsencrypt.org/docs/api/",
      "website": "https://letsencrypt.org",
      "tags": ["免费SSL", "自动化", "开源"]
    },
    {
      "id": "ssl-002",
      "name": "Cloudflare SSL",
      "description": "Cloudflare提供的SSL证书服务，支持多种SSL模式",
      "endpoint": "https://api.cloudflare.com/client/v4/zones/{zone_id}/ssl",
      "method": "GET",
      "parameters": {
        "zone_id": "域名区域ID",
        "X-Auth-Key": "API密钥",
        "X-Auth-Email": "邮箱地址"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://developers.cloudflare.com/api/",
      "website": "https://www.cloudflare.com",
      "tags": ["免费SSL", "CDN集成", "多种模式"]
    },
    {
      "id": "ssl-003",
      "name": "AWS Certificate Manager",
      "description": "AWS托管的SSL证书服务，自动续期和部署",
      "endpoint": "https://acm.{region}.amazonaws.com/",
      "method": "POST",
      "parameters": {
        "Action": "API操作",
        "Version": "API版本",
        "DomainName": "域名",
        "ValidationMethod": "验证方法",
        "SubjectAlternativeNames": "备用域名"
      },
      "response_format": "JSON",
      "rate_limit": "根据AWS限制",
      "is_free": true,
      "documentation": "https://docs.aws.amazon.com/acm/",
      "website": "https://aws.amazon.com/certificate-manager/",
      "tags": ["AWS集成", "自动续期", "免费使用"]
    },
    {
      "id": "ssl-004",
      "name": "ZeroSSL",
      "description": "提供免费和付费SSL证书，支持多种验证方式",
      "endpoint": "https://api.zerossl.com/certificates",
      "method": "POST",
      "parameters": {
        "access_key": "访问密钥",
        "certificate_domains": "证书域名",
        "certificate_validity_days": "有效期天数",
        "certificate_csr": "CSR内容"
      },
      "response_format": "JSON",
      "rate_limit": "3次/天(免费)",
      "is_free": true,
      "documentation": "https://zerossl.com/documentation/api/",
      "website": "https://zerossl.com",
      "tags": ["免费SSL", "多种验证", "API支持"]
    },
    {
      "id": "ssl-005",
      "name": "Certbot",
      "description": "Let's Encrypt官方客户端，自动化SSL证书管理",
      "endpoint": "命令行工具",
      "method": "CLI命令",
      "parameters": {
        "certonly": "仅获取证书",
        "--webroot": "Web根目录",
        "--standalone": "独立模式",
        "--apache": "Apache集成",
        "--nginx": "Nginx集成"
      },
      "response_format": "证书文件",
      "rate_limit": "300次/3小时",
      "is_free": true,
      "documentation": "https://certbot.eff.org/docs/",
      "website": "https://certbot.eff.org",
      "tags": ["官方客户端", "自动化", "完全免费"]
    }
  ],
  "examples": {
    "request_certificate": {
      "description": "请求SSL证书",
      "endpoint": "POST https://acme-v02.api.letsencrypt.org/acme/new-acct",
      "response": {
        "status": 201,
        "headers": {
          "Location": "https://acme-v02.api.letsencrypt.org/acme/acct/account_id"
        }
      }
    },
    "get_ssl_status": {
      "description": "获取SSL状态",
      "endpoint": "GET https://api.cloudflare.com/client/v4/zones/{zone_id}/ssl",
      "response": {
        "success": true,
        "result": {
          "id": "ssl_id",
          "status": "active",
          "mode": "full"
        }
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "Python",
      "package": "certbot",
      "install": "pip install certbot",
      "usage": "from certbot import main; main.main()"
    },
    {
      "language": "Python",
      "package": "acme",
      "install": "pip install acme",
      "usage": "from acme import client; client = client.ClientV2()"
    },
    {
      "language": "JavaScript",
      "package": "greenlock",
      "install": "npm install greenlock",
      "usage": "const Greenlock = require('greenlock');"
    }
  ]
}
