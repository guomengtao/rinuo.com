    {
      "id": "database-005",
      "name": "Firebase Firestore",
      "description": "谷歌的NoSQL云数据库，支持实时同步和离线支持",
      "endpoint": "https://firestore.googleapis.com/v1/projects/{project}/databases/{database}/documents",
