  "last_updated": "2025-01-20",
  "total_apis": 10,
  "apis": [
    {
      "id": "database-001",
      "name": "Supabase",
      "description": "开源的Firebase替代品，提供PostgreSQL数据库、身份验证、存储等服务",
      "endpoint": "https://{project}.supabase.co/rest/v1/{table}",
      "method": "GET/POST/PUT/DELETE",
      "parameters": {
        "apikey": "API密钥",
        "Authorization": "Bearer {token}",
        "select": "选择字段",
        "filter": "过滤条件",
        "order": "排序"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://supabase.com/docs/reference/javascript",
      "tags": ["PostgreSQL", "开源", "Firebase替代"]
    },
    {
      "id": "database-002",
      "name": "PlanetScale",
      "description": "基于MySQL的Serverless数据库平台，支持分支和部署",
      "endpoint": "https://{region}.connect.psdb.cloud",
      "method": "SQL",
      "parameters": {
        "host": "主机地址",
        "username": "用户名",
        "password": "密码",
        "database": "数据库名"
      },
      "response_format": "SQL Results",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://planetscale.com/docs",
      "tags": ["MySQL", "Serverless", "分支部署"]
    },
    {
      "id": "database-003",
      "name": "Neon",
      "description": "Serverless PostgreSQL服务，支持分支和自动扩展",
      "endpoint": "postgresql://{user}:{password}@{host}/{database}",
      "method": "SQL",
      "parameters": {
        "host": "主机地址",
        "username": "用户名",
        "password": "密码",
        "database": "数据库名"
      },
      "response_format": "SQL Results",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://neon.tech/docs",
