  "examples": {
    "create_record": {
      "description": "创建数据库记录",
      "endpoint": "POST https://{project}.supabase.co/rest/v1/users",
      "request_body": {
        "name": "张三",
        "email": "zhangsan@example.com",
        "age": 25
      },
      "response": {
        "id": 1,
        "name": "张三",
        "email": "zhangsan@example.com",
        "age": 25,
        "created_at": "2025-01-20T10:00:00Z"
      }
    },
    "query_records": {
      "description": "查询数据库记录",
      "endpoint": "GET https://{project}.supabase.co/rest/v1/users?select=name,email&age=gt.20",
      "response": {
        "data": [
          {
            "name": "张三",
            "email": "zhangsan@example.com"
          }
        ]
      }
    }
  },
  "sdk_libraries": [
