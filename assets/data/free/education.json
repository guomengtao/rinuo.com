{
  "category": "教育服务",
  "description": "提供在线教育、学习资源、课程管理等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 5,
  "apis": [
    {
      "id": "education-001",
      "name": "Coursera",
      "description": "全球领先的在线教育平台，提供大学课程和认证",
      "endpoint": "https://api.coursera.org/api/catalog.v1/courses",
      "method": "GET",
      "parameters": {
        "q": "搜索关键词",
        "limit": "结果数量限制",
        "start": "起始位置",
        "fields": "返回字段"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://build.coursera.org/app-platform/catalog/",
      "website": "https://www.coursera.org",
      "tags": ["在线教育", "大学课程", "全球平台"]
    },
    {
      "id": "education-002",
      "name": "edX",
      "description": "哈佛和MIT联合创办的在线教育平台",
      "endpoint": "https://api.edx.org/catalog/v1/catalogs/",
      "method": "GET",
      "parameters": {
        "page": "页码",
        "page_size": "页面大小",
        "search": "搜索关键词",
        "type": "课程类型"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://edx.readthedocs.io/projects/edx-developer-guide/",
      "website": "https://www.edx.org",
      "tags": ["哈佛MIT", "在线课程", "高等教育"]
    },
    {
      "id": "education-003",
      "name": "Khan Academy",
      "description": "免费的教育资源平台，提供数学、科学等课程",
      "endpoint": "https://www.khanacademy.org/api/v1/topics",
      "method": "GET",
      "parameters": {
        "kind": "内容类型",
        "domain": "学科领域",
        "limit": "数量限制"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://github.com/Khan/khan-api",
      "website": "https://www.khanacademy.org",
      "tags": ["免费教育", "数学科学", "开放资源"]
    },
    {
      "id": "education-004",
      "name": "Udemy",
      "description": "技能提升平台，提供职业技能和兴趣课程",
      "endpoint": "https://www.udemy.com/api-2.0/courses/",
      "method": "GET",
      "parameters": {
        "search": "搜索关键词",
        "page": "页码",
        "page_size": "页面大小",
        "language": "语言",
        "category": "分类"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://www.udemy.com/developers/",
      "website": "https://www.udemy.com",
      "tags": ["技能提升", "职业培训", "兴趣课程"]
    },
    {
      "id": "education-005",
      "name": "Duolingo",
      "description": "语言学习应用，支持多种语言学习",
      "endpoint": "https://www.duolingo.com/api/1/users/show",
      "method": "GET",
      "parameters": {
        "username": "用户名",
        "fields": "返回字段"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://www.duolingo.com/developers",
      "website": "https://www.duolingo.com",
      "tags": ["语言学习", "多语言", "游戏化"]
    }
  ],
  "examples": {
    "search_courses": {
      "description": "搜索课程",
      "endpoint": "GET https://api.coursera.org/api/catalog.v1/courses?q=python",
      "response": {
        "courses": [
          {
            "id": 12345,
            "name": "Python for Everybody",
            "shortName": "python",
            "language": "en"
          }
        ]
      }
    },
    "get_user_progress": {
      "description": "获取用户学习进度",
      "endpoint": "GET https://www.duolingo.com/api/1/users/show?username=testuser",
      "response": {
        "username": "testuser",
        "languages": [
          {
            "language": "es",
            "level": 5,
            "points": 1250
          }
        ]
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "Python",
      "package": "coursera",
      "install": "pip install coursera",
      "usage": "import coursera"
    },
    {
      "language": "JavaScript",
      "package": "edx-api",
      "install": "npm install edx-api",
      "usage": "const EdxAPI = require('edx-api');"
    }
  ]
}
