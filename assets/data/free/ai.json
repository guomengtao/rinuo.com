  "examples": {
    "chat_completion": {
      "description": "AI对话完成",
      "endpoint": "POST https://api.openai.com/v1/chat/completions",
      "request_body": {
        "model": "gpt-3.5-turbo",
        "messages": [
          {"role": "user", "content": "你好，请介绍一下人工智能"}
        ],
        "max_tokens": 1000
      },
      "response": {
        "choices": [
          {
            "message": {
              "content": "人工智能(AI)是计算机科学的一个分支...",
              "role": "assistant"
            }
          }
        ]
      }
    },
    "image_generation": {
      "description": "AI图像生成",
      "endpoint": "POST https://api.stability.ai/v1/generation/stable-diffusion-xl-1024-v1-0/text-to-image",
      "request_body": {
        "text_prompts": [{"text": "一只可爱的小猫在花园里玩耍"}],
        "cfg_scale": 7,
        "steps": 30
      },
      "response": {
        "artifacts": [
          {
            "base64": "生成的图像base64数据",
            "seed": 12345
          }
        ]
      }
    }
