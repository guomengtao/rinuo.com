      "parameters": {
        "apiKey": "API密钥",
        "domainName": "域名名称",
        "outputFormat": "输出格式",
        "preferFresh": "优先新鲜数据"
      },
      "response_format": "JSON/XML",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://whois.whoisxmlapi.com/api-documentation",
      "website": "https://whois.whoisxmlapi.com",
      "tags": ["WHOIS查询", "域名信息", "注册数据"]
    },
    {
      "id": "domain-005",
      "name": "Domain.com API",
      "description": "域名注册和管理服务的API接口",
      "endpoint": "https://api.domain.com/v1/domains",
      "method": "GET/POST",
      "parameters": {
        "Authorization": "Bearer {access_token}",
