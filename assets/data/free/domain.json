      "endpoint": "https://api.godaddy.com/v1/domains",
      "method": "GET/POST/PUT/DELETE",
      "parameters": {
        "Authorization": "sso-key {api_key}:{api_secret}",
        "X-Shopper-Id": "购物者ID",
        "domain": "域名名称",
        "status": "域名状态"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
