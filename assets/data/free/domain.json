    {
      "id": "domain-006",
      "name": "FreeDNS",
      "description": "提供免费DNS托管服务，支持动态DNS更新",
      "endpoint": "https://freedns.afraid.org/nic/update",
      "method": "GET",
      "parameters": {
        "hostname": "主机名",
        "myip": "IP地址",
        "username": "用户名",
        "password": "密码"
      },
      "response_format": "Text",
      "rate_limit": "无限制",
      "is_free": true,
      "documentation": "https://freedns.afraid.org/",
      "website": "https://freedns.afraid.org",
      "tags": ["免费DNS", "动态DNS", "托管服务"]
    }
  ],
  "examples": {
    "check_domain": {
      "description": "检查域名可用性",
      "endpoint": "GET https://api.godaddy.com/v1/domains/available?domain=example.com",
      "response": {
        "available": true,
        "currency": "USD",
        "definitive": true,
        "domain": "example.com",
        "period": 1,
        "price": 11990000
      }
    },
    "update_dns": {
      "description": "更新动态DNS",
      "endpoint": "GET https://freedns.afraid.org/nic/update?hostname=test.example.com&myip=192.168.1.1",
      "response": "good 192.168.1.1"
    }
  },
  "sdk_libraries": [
