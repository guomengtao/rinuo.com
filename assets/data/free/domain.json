    {
      "id": "domain-001",
      "name": "Cloudflare Registrar",
      "description": "Cloudflare的域名注册服务，提供成本价域名注册",
      "endpoint": "https://api.cloudflare.com/client/v4/zones/{zone_id}/registrar/domains",
      "method": "GET/POST",
      "parameters": {
        "Authorization": "Bearer {api_token}",
        "name": "域名名称",
        "privacy": "隐私保护",
        "auto_renew": "自动续费"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://developers.cloudflare.com/registrar/",
      "website": "https://www.cloudflare.com/products/registrar/",
      "tags": ["域名注册", "成本价", "隐私保护"]
    },
    {
      "id": "domain-002",
      "name": "Namecheap",
      "description": "提供域名注册、托管、SSL证书等服务",
      "endpoint": "https://api.sandbox.namecheap.com/xml.response",
      "method": "GET",
      "parameters": {
        "ApiUser": "API用户名",
        "ApiKey": "API密钥",
