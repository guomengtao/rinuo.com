{
  "category": "图像服务",
  "description": "提供图像处理、图像优化、图像分析等服务的API接口",
  "version": "1.0.0",
  "last_updated": "2025-01-20",
  "total_apis": 6,
  "apis": [
    {
      "id": "image-001",
      "name": "Cloudinary",
      "description": "云图像管理服务，提供上传、转换、优化等功能",
      "endpoint": "https://api.cloudinary.com/v1_1/{cloud_name}/image/upload",
      "method": "POST",
      "parameters": {
        "api_key": "API密钥",
        "timestamp": "时间戳",
        "signature": "签名",
        "file": "图像文件",
        "transformation": "转换参数"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://cloudinary.com/documentation",
      "website": "https://cloudinary.com",
      "tags": ["云管理", "图像转换", "免费套餐"]
    },
    {
      "id": "image-002",
      "name": "Imgix",
      "description": "实时图像处理服务，提供URL参数化图像转换",
      "endpoint": "https://{subdomain}.imgix.net/{path}",
      "method": "GET",
      "parameters": {
        "w": "宽度",
        "h": "高度",
        "fit": "适配模式",
        "q": "质量",
        "fm": "格式"
      },
      "response_format": "Image",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://docs.imgix.com/",
      "website": "https://www.imgix.com",
      "tags": ["实时处理", "URL参数", "高性能"]
    },
    {
      "id": "image-003",
      "name": "ImageKit",
      "description": "智能图像优化服务，支持自动格式选择和压缩",
      "endpoint": "https://ik.imagekit.io/{imagekit_id}/{path}",
      "method": "GET",
      "parameters": {
        "tr": "转换参数",
        "w": "宽度",
        "h": "高度",
        "q": "质量",
        "f": "格式"
      },
      "response_format": "Image",
      "rate_limit": "根据套餐",
      "is_free": true,
      "documentation": "https://docs.imagekit.io/",
      "website": "https://imagekit.io",
      "tags": ["智能优化", "自动压缩", "免费套餐"]
    },
    {
      "id": "image-004",
      "name": "Cloudflare Images",
      "description": "Cloudflare的图像存储和优化服务",
      "endpoint": "https://api.cloudflare.com/client/v4/accounts/{account_id}/images/v1",
      "method": "POST",
      "parameters": {
        "Authorization": "Bearer {api_token}",
        "Content-Type": "multipart/form-data",
        "file": "图像文件",
        "metadata": "元数据"
      },
      "response_format": "JSON",
      "rate_limit": "根据套餐",
      "is_free": false,
      "documentation": "https://developers.cloudflare.com/images/",
      "website": "https://www.cloudflare.com/products/cloudflare-images/",
      "tags": ["Cloudflare", "图像存储", "CDN优化"]
    },
    {
      "id": "image-005",
      "name": "Unsplash API",
      "description": "高质量免费图像库，提供数百万张专业照片",
      "endpoint": "https://api.unsplash.com/search/photos",
      "method": "GET",
      "parameters": {
        "Authorization": "Client-ID {access_key}",
        "query": "搜索关键词",
        "page": "页码",
        "per_page": "每页数量",
        "orientation": "方向"
      },
      "response_format": "JSON",
      "rate_limit": "5000次/小时(免费)",
      "is_free": true,
      "documentation": "https://unsplash.com/developers",
      "website": "https://unsplash.com",
      "tags": ["免费图像", "高质量", "专业照片"]
    },
    {
      "id": "image-006",
      "name": "Pexels API",
      "description": "免费图像和视频库，提供CC0授权的素材",
      "endpoint": "https://api.pexels.com/v1/search",
      "method": "GET",
      "parameters": {
        "Authorization": "{api_key}",
        "query": "搜索关键词",
        "page": "页码",
        "per_page": "每页数量",
        "orientation": "方向"
      },
      "response_format": "JSON",
      "rate_limit": "200次/小时(免费)",
      "is_free": true,
      "documentation": "https://www.pexels.com/api/",
      "website": "https://www.pexels.com",
      "tags": ["免费素材", "CC0授权", "图像视频"]
    }
  ],
  "examples": {
    "upload_image": {
      "description": "上传图像到Cloudinary",
      "endpoint": "POST https://api.cloudinary.com/v1_1/cloud_name/image/upload",
      "request_body": {
        "file": "图像文件",
        "transformation": "w_300,h_200,c_fill"
      },
      "response": {
        "public_id": "image_id",
        "url": "https://res.cloudinary.com/cloud_name/image/upload/v123/image.jpg"
      }
    },
    "search_images": {
      "description": "搜索Unsplash图像",
      "endpoint": "GET https://api.unsplash.com/search/photos?query=nature",
      "response": {
        "results": [
          {
            "id": "image_id",
            "urls": {"regular": "https://images.unsplash.com/photo-123"},
            "user": {"name": "Photographer Name"}
          }
        ]
      }
    }
  },
  "sdk_libraries": [
    {
      "language": "JavaScript",
      "package": "cloudinary",
      "install": "npm install cloudinary",
      "usage": "const cloudinary = require('cloudinary').v2;"
    },
    {
      "language": "Python",
      "package": "cloudinary",
      "install": "pip install cloudinary",
      "usage": "import cloudinary"
    }
  ]
}
